    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "autoprefixer": "^10.4.24",
    "axios": "^1.6.5",
    "date-fns": "^2.30.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.10.3",
    "tailwindcss": "^3.3.6",
    "zod": "^3.22.4",
    "zustand": "^4.4.0"
