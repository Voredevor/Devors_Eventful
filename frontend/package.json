{
  "name": "eventful-frontend",
  "version": "1.0.0",
  "description": "Eventful - Event Ticketing Platform Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx,css}",
